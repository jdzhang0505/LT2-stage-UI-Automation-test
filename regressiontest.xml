<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Regression test">

	<test name="Register test" enabled="true">
		<parameter name="platform" value="windows" />
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="nopcommerce4.LT2.test.RegisterTest"></class>
			<class name="nopcommerce4.LT2.test.PlaceOrderTest"></class>
			<class name="nopcommerce4.LT2.test.MyAccountTest"></class>
			<class name="nopcommerce4.LT2.test.ForgotPasswordTest"></class>
		</classes>
	</test>
	<test name="PlaceOrder test" enabled="false">
		<parameter name="platform" value="windows" />
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="nopcommerce4.LT2.test.PlaceOrderTest"></class>
		</classes>
	</test>
	<test name="MyAccount test" enabled="false">
	    <parameter name="platform" value="windows" />
	    <parameter name="browser" value="chrome" />
	    <classes>
	        <class name="nopcommerce4.LT2.test.MyAccountTest"></class>
	    </classes>
	</test>
	<test name="ForgotPwd test" enabled="false">
		<parameter name="platform" value="windows" />
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="nopcommerce4.LT2.test.ForgotPasswordTest"></class>
		</classes>
	</test>
</suite>